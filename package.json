{
  "name": "@teamhive/capacitor-video-recorder",
  "version": "4.0.1",
  "description": "",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "removePacked": "rm -f teamhive-capacitor-video-recorder-*.tgz",
    "publishLocally": "npm run removePacked && npm run build && npm pack",
    "release": "standard-version -a",
    "release:alpha": "standard-version -a --prerelease alpha",
    "release:prerelease": "standard-version -a --prerelease",
    "release:preview": "standard-version --dry-run"
  },
  "author": "Sean Bannigan",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/android": "^2.3.0",
    "@capacitor/core": "^2.3.0",
    "@capacitor/ios": "^2.3.0",
    "standard-version": "^9.3.0",
    "typescript": "^2.6.2"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "TeamhiveCapacitorVideoRecorder.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vizeke/capacitor-video-recorder.git"
  },
  "bugs": {
    "url": "https://github.com/vizeke/capacitor-video-recorder/issues"
  }
}
